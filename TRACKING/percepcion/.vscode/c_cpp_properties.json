{
  "configurations": [
    {
      "name": "ROS2",
      "includePath": [
        "${workspaceFolder}/**",
        "${workspaceFolder}/install/**/include",
        "/opt/ros/humble/include/**",
        "/usr/include/**",
        "/usr/include/opencv4/**"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/**",
          "${workspaceFolder}/install/**/include",
          "/opt/ros/humble/include/**"
        ],
        "limitSymbolsToIncludedHeaders": false,
        "databaseFilename": ""
      },
      "defines": [],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "gnu11",
      "cppStandard": "c++17",
      "intelliSenseMode": "linux-gcc-x64"
    }
  ],
  "version": 4
}
